        "vue": "^3.2.37",
        "vue-axios": "^3.4.1",
        "vue-currency-input": "^3.0.0",
        "vue-i18n": "^9.1.10",
        "vue-matomo": "^4.2.0",
        "vue-router": "^4.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.18.9",
