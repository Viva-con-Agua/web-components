        "eslint-plugin-storybook": "^0.6.1",
        "eslint-plugin-vue": "^9.2.0",
        "gh-pages": "^4.0.0",
        "jsdom": "^20.0.0",
        "postcss": "^8.4.16",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "sass": "^1.53.0",
